{ 
    "manifest_version": 3,
    "name": "Scraping Test",
    "version": "1", 
    "permissions": ["declarativeContent", "activeTab", "webNavigation"],
    "background.service_worker": {
        "scripts": ["background.js"]
      },
      "action": {
        "default_icon": "icon.png" 
      },
    "content_scripts": [
        {
            "matches": [
              "*://www.instacart.com/store/*"
            ],
            "js": ["jquery-3.6.0.min.js", "content.js"]
        }
    ]
}
