{
	"name": "Sexy Tofu Chrome Plugin",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "./js/background.js"
	},
	"permissions": [
		"storage"
	],
	"action": {
		"default_icon": {},
		"default_popup": "./popup/no-food.html",
		"default_title": "Sexy Tofu"
	},
	"icons": {
		"16": "./img/16x16.png",
		"48": "./img/48x48.png",
		"128": "./img/128x128.png"
	},
	"content_scripts": [
		{
			"matches": [
				"*://*/*/demo.html",
				"file://*/*/demo.html",
				"*://www.instacart.com/store/*"
			],
			"js": [
				"./js/jquery-3.6.0.min.js",
				"./js/common.js",
				"./js/content.js"
			]
		}
	]
}